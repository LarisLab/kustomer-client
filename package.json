{
    "name": "kustomer-client",
    "version": "1.0.6",
    "description": "Kustomer API client",
    "keywords": [
        "kustomer",
        "api",
        "rest",
        "client"
    ],
    "homepage": "https://github.com/LarisLab/kustomer-client",
    "bugs": {
        "url": "https://github.com/LarisLab/kustomer-client/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/LarisLab/kustomer-client.git"
    },
    "license": "MIT",
    "author": "Laris Lab",
    "sideEffects": false,
    "type": "module",
    "main": "./dist/bundle.cjs",
    "module": "./dist/bundle.module.mjs",
    "unpkg": "./dist/bundle.umd.js",
    "exports": {
        "import": "./dist/bundle.modern.mjs",
        "require": "./dist/bundle.cjs",
        "default": "./dist/bundle.modern.mjs",
        "types": "./dist/index.d.ts"
    },
    "source": "./src/index.ts",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "microbundle && typedoc src/index.ts",
        "generate": "openapi-ts"
    },
    "browserslist": [
        "defaults"
    ],
    "prettier": {
        "semi": false,
        "singleQuote": true
    },
    "release": {
        "branches": [
            "+([0-9])?(.{+([0-9]),x}).x",
            "main",
            "next",
            "next-major",
            {
                "name": "beta",
                "prerelease": true
            },
            {
                "name": "alpha",
                "prerelease": true
            }
        ],
        "plugins": [
            "@semantic-release/commit-analyzer",
            "@semantic-release/release-notes-generator",
            "@semantic-release/changelog",
            "@semantic-release/npm",
            "@semantic-release/git",
            "@semantic-release/github"
        ]
    },
    "devDependencies": {
        "@hey-api/openapi-ts": "^0.67.4",
        "microbundle": "^0.15.1",
        "typedoc": "^0.28.4",
        "typescript": "^5.8.3"
    },
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "@hey-api/client-fetch": "^0.10.1"
    }
}